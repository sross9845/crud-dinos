<ul>
    <% dinos.forEach(function(dino, id){ %>
        <li>
            <a href="/dinosaurs/<%= id %>"><%= dino.name %></a> is a <%= dino.type %>
            <form action="/dinosaurs/<%= id %>/?_method=DELETE" method="POST">
            <input type="submit" value="X">
            </form>
        </li>
    <% }); %>
</ul>

